{
  "name": "typescript-estree-cli",
  "version": "0.0.2",
  "description": "Cli tool for generating AST from typescript source files",
  "main": "main.js",
  "scripts": {
    "start": "node build/main.js",
    "build": "tsc",
    "build:release": "tsc && webpack",
    "clean": "rimraf dist && rimraf build",
    "pack": "pkg .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "macdeluck",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^4.26.1",
    "fs": "^0.0.1-security",
    "ts-command-line-args": "^2.0.1"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
  },
  "bin": {
    "typescript-estree-cli": "./build/main.js"
  },
  "pkg": {
    "scripts": "build/*.js",
    "targets": [
      "node14-linux-x64",
      "node14-win-x64",
      "node14-osx-x64"
    ],
    "outputPath": "dist"
  }
}
