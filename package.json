{
  "name": "typescript-estree-cli",
  "version": "0.0.1",
  "description": "Cli tool for generating AST from typescript source files",
  "main": "main.js",
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc",
    "pack": "pkg .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "macdeluck",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/typescript-estree": "^4.26.1",
    "fs": "^0.0.1-security",
    "ts-command-line-args": "^2.0.1",
    "typescript": "^4.3.2"
  },
  "devDependencies": {
    "@types/node": "^15.12.2"
  },
  "bin": {
    "typescript-estree-cli": "./dist/main.js"
  },
  "pkg": {
    "scripts": "dist/*.js",
    "targets": [
      "node14-linux-x64",
      "node14-win-x64",
      "node14-osx-x64"
    ],
    "outputPath": "dist"
  }
}
